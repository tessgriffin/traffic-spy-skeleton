<h1><%= @source.identifier %></h1>

<h2>Visited URLS</h2>
<ul class="urls">
  <% @source.ordered_urls.each do |url| %>
  <li><%= url %></li>
  <% end %>
</ul>

<h2>Average Reponse Times</h2>
<ul class="average_urls">
  <% @source.ordered_url_response_times.each do |url| %>
  <li><%= url %></li>
  <% end %>
</ul>

<h2>Browsers</h2>
<ul class="browsers">
  <% @source.browsers.each do |browser| %>
  <li><%= browser %></li>
  <% end %>
</ul>


<h2>Platforms</h2>
<ul class="platforms">
  <% @source.platforms.each do |platform| %>
  <li><%= platform %></li>
  <% end %>
</ul>
